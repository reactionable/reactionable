{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "jest",
          "test",
          "test:coverage",
          "build",
          "build:clean",
          "build:barrelsby",
          "build:docs",
          "lint"
        ]
      }
    }
  },
  "targetDefaults": {
    "start": {
      "dependsOn": ["^start"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/lib"]
    },
    "build:clean": {
      "dependsOn": ["^build:clean"],
      "outputs": ["{projectRoot}/lib"]
    },
    "build:barrelsby": {
      "dependsOn": ["^build:barrelsby"],
      "outputs": ["{projectRoot}/src/index.ts"]
    },
    "test:coverage": {
      "outputs": ["{projectRoot}/coverage"]
    },
    "build:docs": {
      "outputs": ["{projectRoot}/../../build/storybook/docs"]
    }
  }
}
