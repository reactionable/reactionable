    "react": "^16.10.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@reactionable/core": "^0.15.0",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "react-apollo": "^3.1.3"
  },
  "devDependencies": {
