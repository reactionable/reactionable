{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "lib",
    "declarationDir": "lib",
    "maxNodeModuleJsDepth": 10,
  },
  "include": ["src"],
  "typedocOptions": {
    "out": "../../build/storybook/docs/graphql",
    "entryPoints": ["src/index.ts"],
    "hideGenerator": true,
    "githubPages": true,
    "includeVersion": true,
    "readme": "none",
    "excludeExternals": true,
    "excludePrivate": true
  }
}
