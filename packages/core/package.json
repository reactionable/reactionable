{
  "name": "@reactionable/core",
  "version": "0.28.31",
  "description": "Reactionable Core package",
  "files": [
    "lib"
  ],
  "type": "module",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "main": "lib/cjs/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "npm run build:barrelsby && tsc --watch",
    "jest": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --forceExit",
    "test": "npm run jest -- --maxWorkers=50%",
    "test:coverage": "npm run jest -- --coverage",
    "build:pre": "npm run build:clean && npm run build:barrelsby",
    "build": "npm run build:pre && npm run build:cjs && npm run build:esm",
    "build:ci": "npm run build:pre && npm run build:cjs && npm run build:esm",
    "build:clean": "rimraf lib",
    "build:barrelsby": "npm exec barrelsby -- --noHeader -d src --exclude \".+\\.test\\..+\" --exclude \".+\\.stories\\..+\" --delete -q && npm exec prettier -- --write src/index.ts",
    "build:cjs": "mkdir -p lib/cjs && echo '{\"type\": \"commonjs\"}' > lib/cjs/package.json && tsc --pretty -p tsconfig.build.json --module commonjs --outDir lib/cjs/",
    "build:esm": "mkdir -p lib/esm && echo '{\"type\": \"module\"}' > lib/esm/package.json && tsc --pretty -p tsconfig.build.json --module esnext --outDir lib/esm/",
    "build:docs": "typedoc --tsconfig tsconfig.build.json",
    "lint": "eslint \"{src,stories}/**/*.{ts,tsx}\"",
    "lint:ci": "npm run lint -- --output-file eslint-report.json --format json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactionable/reactionable.git"
  },
  "keywords": [
    "reactionable",
    "react",
    "framework",
    "core"
  ],
  "author": "ESCEMI <developer@escemi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactionable/reactionable/issues"
  },
  "homepage": "https://github.com/reactionable/reactionable#readme",
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "formik": "^2.4.9",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "path-to-regexp": "^8.3.0",
    "react-fast-compare": "^3.2.0",
    "react-i18next": "^16.5.1",
    "yup": "^1.7.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1301e652cf34ddf0e2e42509f49b059f34c492a9",
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "jsdom",
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true,
        "tsconfig": "../../tsconfig.jest.json",
        "isolatedModules": true,
        "diagnostics": false
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../../__tests__/jest.setup.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ]
  }
}
